{
    "version": 2,
    "name": "intesa-vincente",
    "buildCommand": "npm run start",
    "installCommand": "npm install",
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.js",
            "methods": ["GET", "POST", "PUT", "DELETE"]
        }
    ]
}